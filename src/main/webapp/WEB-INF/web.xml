<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>Washington-Time</servlet-name>
    <servlet-class>com.CityTime.WashingtonTime</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Years-Old</servlet-name>
    <servlet-class>com.CityTime.YearsOld.Age</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>Washington-Time</servlet-name>
    <url-pattern>/washington</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Years-Old</servlet-name>
    <url-pattern>/age</url-pattern>
  </servlet-mapping>
</web-app>
